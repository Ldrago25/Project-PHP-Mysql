SELECT u.Nombre, u.Apellido, u.Cedula, b.Nombre AS 'Evento', b.Ubicacion FROM boleto b INNER JOIN usuario u ON b.usuario_Id = u.Id_Usuario WHERE B.usuario_Id = '1';

